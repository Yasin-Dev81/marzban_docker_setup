{
    "log": {
        "loglevel": "info"
    },
    "inbounds": [
        {
            "tag": "Vless_WS_Inbound",
            "listen": "127.0.0.1",
            "port": 2012,
            "protocol": "vless",
            "settings": {
              "clients": [],
              "decryption": "none"
            },
            "streamSettings": {
              "network": "ws",
              "security": "none",
              "wsSettings": {
                "path": "/marzban/me/2012"
              }
            },
            "sniffing": {
              "enabled": true,
              "destOverride": [
                "http",
                "tls"
              ]
            }
        }
    ],
    "outbounds": [
        {
            "tag": "freedom",
            "protocol": "freedom",
            "settings": {}
        },
        {
            "protocol": "blackhole",
            "tag": "blackhole"
        },
        {
            "tag": "DNS-Internal",
            "protocol": "dns",
            "settings": {
                "address": "127.0.0.53",
                "port": 53
            }
        }
    ],
 "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "type": "field",
                "outboundTag": "blackhole",
                "ip": [
                    "geoip:private",
                    "geoip:ir"
                ]
            },
            {
                "type": "field",
                "port": 53,
                "network": "tcp,udp",
                "outboundTag": "DNS-Internal"
            },
            {
                "type": "field",
                "outboundTag": "blackhole",
                "protocol": [
                    "bittorrent"
                ]
            },
            {
                "outboundTag": "blackhole",
                "domain": [
                    "regexp:.*\\.ir$",
                    "ext:iran.dat:ir",
                    "ext:iran.dat:other",
                    "geosite:category-ir",
                    "blogfa",
                    "bank",
                    "tebyan.net",
                    "beytoote.com",
                    "Film2movie.ws",
                    "Setare.com",
                    "downloadha.com",
                    "Sanjesh.org"
                ],
                "type": "field"
            }
        ]
    }
}
